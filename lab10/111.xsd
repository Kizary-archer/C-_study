<?xml version="1.0" standalone="yes"?>
<xs:schema id="warehouseDataSet" targetNamespace="http://tempuri.org/warehouseDataSet.xsd" xmlns:mstns="http://tempuri.org/warehouseDataSet.xsd" xmlns="http://tempuri.org/warehouseDataSet.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="warehouseDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="clients">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_passport" type="xs:int" minOccurs="0" />
              <xs:element name="id_client" type="xs:int" />
              <xs:element name="name" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="surname" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="patronymic" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="phone" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="contracts">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_contracts" type="xs:int" />
              <xs:element name="id_client" type="xs:int" minOccurs="0" />
              <xs:element name="id_tariffs" type="xs:int" minOccurs="0" />
              <xs:element name="id_status" type="xs:int" minOccurs="0" />
              <xs:element name="date_of_conclusion" type="xs:dateTime" minOccurs="0" />
              <xs:element name="status" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="passport">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_passport" type="xs:int" />
              <xs:element name="id_client" type="xs:int" minOccurs="0" />
              <xs:element name="Date_issues" type="xs:dateTime" minOccurs="0" />
              <xs:element name="Date_of_birth" type="xs:dateTime" minOccurs="0" />
              <xs:element name="issued_by" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="product">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_cell" type="xs:int" />
              <xs:element name="id_product" type="xs:int" />
              <xs:element name="id_client" type="xs:int" minOccurs="0" />
              <xs:element name="name_product" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="number_product" type="xs:int" minOccurs="0" />
              <xs:element name="id_contracts" type="xs:int" minOccurs="0" />
              <xs:element name="id_cells" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="status_contracts">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_contracts" type="xs:int" minOccurs="0" />
              <xs:element name="id_status" type="xs:int" />
              <xs:element name="status" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="storage_cells">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_cell" type="xs:int" />
              <xs:element name="name_cell" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="tariffs">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="id_tariffs" type="xs:int" />
              <xs:element name="name_tariffs" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="description" minOccurs="0">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:maxLength value="100" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="price" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="countClients">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Column1" msdata:ReadOnly="true" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:clients" />
      <xs:field xpath="mstns:id_client" />
    </xs:unique>
    <xs:unique name="contracts_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:contracts" />
      <xs:field xpath="mstns:id_contracts" />
    </xs:unique>
    <xs:unique name="passport_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:passport" />
      <xs:field xpath="mstns:id_passport" />
    </xs:unique>
    <xs:unique name="product_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:product" />
      <xs:field xpath="mstns:id_cell" />
      <xs:field xpath="mstns:id_product" />
    </xs:unique>
    <xs:unique name="status_contracts_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:status_contracts" />
      <xs:field xpath="mstns:id_status" />
    </xs:unique>
    <xs:unique name="storage_cells_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:storage_cells" />
      <xs:field xpath="mstns:id_cell" />
    </xs:unique>
    <xs:unique name="tariffs_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:tariffs" />
      <xs:field xpath="mstns:id_tariffs" />
    </xs:unique>
  </xs:element>
  <xs:annotation>
    <xs:appinfo>
      <msdata:Relationship name="FK_CONTRACT_ЕСТЬ_TARIFFS" msdata:parent="tariffs" msdata:child="contracts" msdata:parentkey="id_tariffs" msdata:childkey="id_tariffs" />
      <msdata:Relationship name="FK_CONTRACT_ЗАКЛЮЧАЕТ_CLIENTS" msdata:parent="clients" msdata:child="contracts" msdata:parentkey="id_client" msdata:childkey="id_client" />
      <msdata:Relationship name="FK_CONTRACT_СОДЕРЖИТ_STATUS_C" msdata:parent="status_contracts" msdata:child="contracts" msdata:parentkey="id_status" msdata:childkey="id_status" />
      <msdata:Relationship name="FK_PASSPORT_RELATIONS_CLIENTS" msdata:parent="clients" msdata:child="passport" msdata:parentkey="id_client" msdata:childkey="id_client" />
      <msdata:Relationship name="FK_PRODUCT_В_CONTRACT" msdata:parent="contracts" msdata:child="product" msdata:parentkey="id_contracts" msdata:childkey="id_contracts" />
      <msdata:Relationship name="FK_PRODUCT_ПРИНАДЛЕЖ_CLIENTS" msdata:parent="clients" msdata:child="product" msdata:parentkey="id_client" msdata:childkey="id_client" />
      <msdata:Relationship name="FK_PRODUCT_ХРАНИТСЯ_STORAGE_" msdata:parent="storage_cells" msdata:child="product" msdata:parentkey="id_cell" msdata:childkey="id_cell" />
    </xs:appinfo>
  </xs:annotation>
</xs:schema>